#include <stdio.h>
#include <stdlib.h>

// Funcao principal do programa

void main () {
    
    int vetor [3];
    
    // Passando valores para o vetor
    
    vetor [0] = 5;
    vetor [1] = 10;
    vetor[2] = 15;
    
    // Exibindo os valores
    printf ("\nPosicao 0: %d", vetor [0]);
    printf ("\nPosicao 1: %d", vetor [1]);
    printf ("\nPosicao 2: %d", vetor [2]);
    
    
}
